{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "react/forbid-prop-types": "off",
        "no-restricted-exports": "off",
        "import/prefer-default-export": "off"
    }
}
